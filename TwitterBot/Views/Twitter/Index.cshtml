@model TwitterBot.Models.ViewModels.TwitterX

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Test</title>
</head>
<body>
    <div>
        <h2>Friends</h2>
        <ul>
            @foreach (var item in Model.Friendship.Users)
            {
                <li>@@@item.ScreenNameResponse</li>
            }
        </ul>
        <h2>Followers</h2>
        <ul>
            @foreach (var item in Model.Followers.Users)
            {
                <li>@@@item.ScreenNameResponse</li>
            }
        </ul>

    </div>
</body>
</html>